type Project {
    id: ID!
    name: String!
    users: [User]!
    createdAt: String!
    gridLayouts: [GridLayout]!
}

extend type Query {
    getProject(projectId: ID!): Project!
}

extend type Mutation {
    addGridLayoutToProject(gridLayoutId: ID!, projectId: ID!): Project!
    newProject(userId: ID!): Project!
}